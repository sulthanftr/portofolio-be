// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Experience {
  id          Int      @id @default(autoincrement())
  title       String   @unique
  description String
  startedAt   DateTime @default(now())
  endedAt     DateTime?
}

model User {
  id          Int      @id @default(autoincrement())
  name        String   
  email       String   @unique
  password    String  
}